    function makeSelectItemByValue(selectId, sOptionValue) {
        var oSelect = document.getElementById(selectId);
        for (index = 0; index < oSelect.length; index++) {
            if (oSelect[index].value == sOptionValue) {
                oSelect[index].selected = true;
                break;
            }
        }
    }
    var i = 0;
